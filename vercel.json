{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "yarn install && yarn build",
  "outputDirectory": "build",
  "installCommand": "yarn install",
  "framework": "create-react-app",
  "functions": {},
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096"
    }
  }
}